<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tab"
    app:startDestination="@id/moreFragment">

    <fragment
        android:id="@+id/editProfileFragment4"
        android:name="com.example.eurisko_challenge.Fragments.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/moreFragment"
        android:name="com.example.eurisko_challenge.Fragments.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" >
        <action
            android:id="@+id/action_moreFragment_to_changePassFragment3"
            app:destination="@id/changePassFragment3" />
        <action
            android:id="@+id/action_moreFragment_to_aboutUsFragment2"
            app:destination="@id/aboutUsFragment2" />
        <action
            android:id="@+id/action_moreFragment_to_editProfileFragment4"
            app:destination="@id/editProfileFragment4" />
        <action
            android:id="@+id/action_moreFragment_to_newFragment22"
            app:destination="@id/newFragment2" />
    </fragment>
    <fragment
        android:id="@+id/changePassFragment3"
        android:name="com.example.eurisko_challenge.Fragments.ChangePassFragment"
        android:label="ChangePassFragment" />
    <fragment
        android:id="@+id/aboutUsFragment2"
        android:name="com.example.eurisko_challenge.Fragments.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/newFragment2"
        android:name="com.example.eurisko_challenge.Fragments.NewFragment"
        android:label="fragment_new"
        tools:layout="@layout/fragment_new" />
